import java.util.ArrayList;
import java.util.List;

public class TestClass {
	public static void main(String[] args) {
		int intPosition1 = 1001;
		System.out.println(intPosition1 % 10);
		intPosition1  = intPosition1 / 10;
		System.out.println(intPosition1 % 10);
		intPosition1  = intPosition1 / 10;
		System.out.println(intPosition1 % 10);
		intPosition1  = intPosition1 / 10;
		System.out.println(intPosition1 % 10);
		intPosition1  = intPosition1 / 10;
	}
}
